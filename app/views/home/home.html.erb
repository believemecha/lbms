
<div class="container">
    <!-- Pagination -->
    <div class="text-center w-100">
    <%= paginate @call_logs %>
    </div>
  <h3> Hi,<%=current_user.email%>  </h3>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>ID</th>
        <th>Phone Number</th>
        <th>Call Start Time</th>
        <th>Call End Time</th>
        <th>Duration (seconds)</th>
        <th>Name</th>
        <!-- Add more headers if needed -->
      </tr>
    </thead>
    <tbody>
      <% @call_logs.each do |call_log| %>
        <tr>
          <td><%= call_log.id %></td>
          <td><%= call_log.phone_number %></td>
          <td><%= call_log.call_start_time %></td>
          <td><%= call_log.call_end_time %></td>
          <td><%= call_log.duration %></td>
          <td><%= call_log.name %></td>
          <!-- Add more columns if needed -->
        </tr>
      <% end %>
    
    </tbody>
  </table>
    <%  if !@call_logs.present? %>
    <p>Hey we have not your data.</p>
    <% end %>


</div>
